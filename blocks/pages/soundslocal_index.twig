{%- if req.secure != true -%}
    {%- do res.redirect('https://'~req.host~req.path~(req.query ? '?' ~ req.query : ''), 301) -%}
{%- endif -%}
{%- import "@blocks/macros/asset-file.twig" as assetfile -%}
{%- import "@blocks/macros/resize.twig" as resize -%}
{% set article_prop62 = prop62.video_presence(post.contents) %}
{%- set featured = api.data.query({ schema_name: "sl_featured"}).results -%}
{%- set episodes = api.data.query({
        schema_name: "sl_episode",
        sort_by: {
        created: 1
        }
    }).results
-%}
<!DOCTYPE html>
<html>
<head>
{#     {{prettydump(episodes)}} #}
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sounds Local</title>
    <link media="all" rel="stylesheet" href="{{ asset('styles/'~assetfile.assetfile('sl-main.min.css')) }}">
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,200,300,400,500,600,700" rel="stylesheet">
    <link rel="shortcut icon" href="{{asset('images/sl_favicon2.ico')}}" type="image/x-icon">
    <link rel="icon" href="{{asset('images/sl_favicon2.ico')}}" type="image/x-icon">
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js" defer></script>
