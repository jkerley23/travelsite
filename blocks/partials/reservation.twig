<script type="text/javascript">
	function getParameterByName(name) {
	    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	        results = regex.exec(location.search);
	    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}
	var refclickid = getParameterByName('refclickid');
	document.getElementById('hotel_refclickid').value = refclickid;
	document.getElementById('car_refclickid').value = refclickid;
	document.getElementById('air_refclickid').value = refclickid;
	document.getElementById('air_oneway_refclickid').value = refclickid;

	$(document).ready(function() {
		$(".rs_tabs").on("click", function(){
			var futureTab = $(this).data("tab"),
				$selectedForm = $("."+futureTab);
			if ($selectedForm.hasClass("rs_searchbox_hide")) {
				$selectedForm.removeClass('rs_searchbox_hide').siblings("form").addClass("rs_searchbox_hide");
			}
		});
	});
</script>
